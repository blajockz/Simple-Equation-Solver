from cmath import *
print("Write your equation in this form : axÂ²+b=c")
print("Write a specific value for a")
a = float(input())
print("Write a specific value for b")
b = float(input())
print("Write a specific value for c")
c = float(input())
D = b*b - 4*a*c
x1 = -b - sqrt(D)/2*a
x2 = -b + sqrt(D)/2*a
print("The soltuions of this equation are", x1, x2 )
